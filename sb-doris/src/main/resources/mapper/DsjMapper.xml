<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dewen.mapper.DsjMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dewen.entity.Dsj">
        <id column="dsj_id" property="dsjId" />
        <result column="dsj_date" property="dsjDate" />
        <result column="dsj_datetime" property="dsjDatetime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        dsj_id, dsj_date, dsj_datetime
    </sql>

</mapper>
